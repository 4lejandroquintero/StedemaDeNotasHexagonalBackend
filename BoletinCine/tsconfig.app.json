{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@core/*": [
        "src/app/core/*"
      ],
      "@core-service/*": [
        "src/app/core/services/*"
      ],
      "@website/*": [
        "src/app/feature/website/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ]
      ,
      "@cms/*": [
        "src/app/feature/cms/*"
      ],
      "@home/*": [
        "src/app/feature/website/home/*"
      ],
      "@models/*": ["src/app/shared/models/*"]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
